{% extends "LikipeBackendBundle::layout-login.html.twig" %}
{% block title "Access Denied" %}
{% block content_login_backend %}
<div class="container-fluid">
	<div class="row-fluid">
		<div class="row-fluid">
			<div class="span12 center login-header">
				<h2>Symfony training with MongoDB</h2>
			</div><!--/span-->
		</div><!--/row-->

		<div class="row-fluid">
			<div class="well span5 center login-box">
				<div class="alert alert-error">
					<button type="button" class="close" data-dismiss="alert">&times;</button>
					<h4>Warning!</h4>
				</div>
				<div class="header"><span><i class="icon-warning-sign"></i></span><span><header>{% trans %}Access Denied{% endtrans %}</header></span></div>
				<div class="content error">
					{% trans %}You are not authorize to access to this page! Please try to login to another account.{% endtrans %}
					<br/><br/>
					<input class="btn btn-info" type="button" onclick="location.href='{{ path('LikipeBackendBundle_Security_login') }}'" value="{% trans %}Cancel{% endtrans %}" />
				</div>
			</div><!--/span-->
		</div><!--/row-->
	</div><!--/fluid-row-->
</div><!--/.fluid-container-->
{% endblock content_login_backend %}