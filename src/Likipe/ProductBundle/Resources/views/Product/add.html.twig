{% extends "LikipeBackendBundle::layout.html.twig" %}
{% block title "Create product" %}
{% block content %}
<div class="container">

	<div class="row-fluid blog">
		<h1>Create new product!</h1>
		<form class="form-product" action="" method="post" {{ form_enctype(form) }}>
			<div class="content">
			{{ form_errors(form) }}
			<div class="form-content clearfix row-fluid">
				<div class="form-info">
				{{ form_widget(form) }}
				</div>
				{{ form_rest(form) }}
				</div>
				<div class="form-action" style="padding-top: 50px;">
					<input class="btn btn-success" type="submit" value="{% trans %}Save{% endtrans %}" />
					<input class="btn btn-info" type="button" onclick="location.href='{{ path('LikipeProductBundle_Product_index') }}'" value="{% trans %}Cancel{% endtrans %}" />
				</div>
			</div>
		</form>
	</div>

</div> <!-- /container -->
{% endblock %}